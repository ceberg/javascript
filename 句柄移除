<!DOCTYPE html>
<html>
<head lang="en">
	<meta charset="utf-8">
</head>
<body>
	<form>
		<input id="btn" type="button" value="button">
	</form>
	<script>
		var x=document.getElementById("btn")
		x.addEventListener("click",hello);
		x.addEventListener("click",world);
		x.removeEventListener("click",hello);
		
		function hello(){
			alert("hello");}
		function world(){
			alert("world");}
	</script>
</body>
</html>
